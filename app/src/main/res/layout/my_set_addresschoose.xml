<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff" >
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="@dimen/activity_size_45">
		<!--返回按钮的布局-->
		<RelativeLayout
			android:id="@+id/rl_back"
			android:layout_width="@dimen/activity_size_60"
			android:layout_height="match_parent">
			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@mipmap/ic_fanhui_left_red"
				android:layout_centerVertical="true"
				android:layout_marginLeft="@dimen/activity_size_15"/>
		</RelativeLayout>
		<!--标题头文本-->
		<TextView
			android:id="@+id/top_title"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="@dimen/text_size_20"
			android:layout_centerInParent="true"
			android:textColor="@color/black"
			android:text="添加收货地址"
			/>
	</RelativeLayout>
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="1dp"
		android:background="@color/view_color"
		></RelativeLayout>
    <!--<include-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content"-->
    <!--layout="@layout/onback_top" />-->

    <LinearLayout
        android:id="@+id/my_set_adresschoose_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="gone" >
    
	<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/yy_myset_shouhuo01" >
	    
	    <TextView
	android:id="@+id/my_set_adresschoose_textview_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:gravity="center_vertical"
    android:textColor="#f86604"
    android:text="请选择  省区/其他..."
    android:textSize="20sp" />
	    
	</LinearLayout>
	
	<ListView
	android:id="@+id/my_set_adresschoose_listview_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:overScrollMode="never"
    android:divider="@drawable/button_line"
    android:scrollbars="none"
    android:cacheColorHint="@null">
	    
	</ListView>
	</LinearLayout>
	
     <LinearLayout
        android:id="@+id/my_set_adresschoose_2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="gone" >
    
	<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/yy_myset_shouhuo01" >
	    
	    <TextView
	android:id="@+id/my_set_adresschoose_textview_2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:gravity="center_vertical"
    android:textColor="#f86604"
    android:text="请选择  城区/其他..."
    android:textSize="20sp" />
	    
	</LinearLayout>
	
	<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/yy_myset_shouhuo03" >
	    
	    <TextView
	android:id="@+id/my_set_adresschoose_sheng_2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:gravity="center_vertical"
    android:textColor="#36428c"
    android:text="广东省"
    android:textSize="20sp" />
	    
	</LinearLayout>
	
	<ListView
	android:id="@+id/my_set_adresschoose_listview_2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:divider="@drawable/button_line"
    android:scrollbars="none"
    android:overScrollMode="never"
    android:cacheColorHint="@null">
	    
	</ListView>
	</LinearLayout>
	
     <LinearLayout
        android:id="@+id/my_set_adresschoose_3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="visible" >
    
	<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/yy_myset_shouhuo01" >
	    
	    <TextView
	android:id="@+id/my_set_adresschoose_textview_3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:gravity="center_vertical"
    android:textColor="#f86604"
    android:text="请选择  县区/其他..."
    android:textSize="20sp" />
	    
	</LinearLayout>
	
	<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/yy_myset_shouhuo03" >
	    
	    <TextView
	android:id="@+id/my_set_adresschoose_sheng_3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:textColor="#36428c"
    android:gravity="center_vertical"
    android:text="广东省"
    android:textSize="20sp" />
	    
	</LinearLayout>
	
	<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/yy_myset_shouhuo02" >
	    
	    <TextView
	android:id="@+id/my_set_adresschoose_shi_3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:gravity="center_vertical"
    android:textColor="#b20308"
    android:text="广州市"
    android:textSize="20sp" />
	    
	</LinearLayout>
	
	<ListView
	android:id="@+id/my_set_adresschoose_listview_3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:divider="@drawable/button_line"
    android:scrollbars="none"
    android:overScrollMode="never"
    android:cacheColorHint="@null">
	    
	</ListView>
	
	</LinearLayout>

</LinearLayout>
